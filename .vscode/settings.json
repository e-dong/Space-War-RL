{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "editor.rulers": [80],
  "black-formatter.path": ["venv/bin/black"],
  "black-formatter.importStrategy": "fromEnvironment",
  "black-formatter.args": ["--line-length", "80"],
  "isort.args": ["--profile", "black"],
  "isort.path": ["venv/bin/isort"],
  "pylint.path": ["venv/bin/pylint"],
  "pylint.importStrategy": "fromEnvironment",
  "pylint.args": ["--extension-pkg-whitelist=pygame"],
  "flake8.path": ["venv/bin/flake8"],
  "flake8.args": ["--line-length", "80"],
  "flake8.importStrategy": "fromEnvironment",
  "cSpell.words": [
    "arange",
    "centerx",
    "centery",
    "collidelist",
    "colliderect",
    "cooldown",
    "DDPG",
    "deltax",
    "deltay",
    "Dota",
    "elif",
    "endx",
    "endy",
    "gthread",
    "KEYDOWN",
    "keypress",
    "keypresses",
    "KEYUP",
    "kwargs",
    "newrect",
    "newsurf",
    "numpy",
    "pseudocode",
    "pygame",
    "rects",
    "reinforcementlearning",
    "spacewar",
    "spritecollide",
    "startx",
    "starty",
    "USEREVENT",
    "VIDEODRIVER"
  ]
}
